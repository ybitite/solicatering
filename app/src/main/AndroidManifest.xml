<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="website.livingRoom.soliCatering.utile.SoliCateringApplication"
        android:allowBackup="true"
        android:icon="@drawable/logogray"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logogray"
        android:supportsRtl="true"
        android:theme="@style/Theme.SoliCatering">
        <activity
            android:name="website.livingRoom.soliCatering.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>